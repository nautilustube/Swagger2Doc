    "components": {
      "schemas": {
        "CustomerNotification": {
          "type": "object",
          "properties": {
            "serialNumber": {
              "type": "string",
              "description": "通知序號",
              "format": "uuid"
            },
            "projectCode": {
              "type": "string",
              "description": "cake問卷序號",
              "format": "uuid"
            },
            "branch": {
              "type": "string",
              "description": "分公司",
              "nullable": true
            },
            "account": {
              "type": "string",
              "description": "帳號",
              "nullable": true
            },
            "ssn": {
              "type": "string",
              "description": "身分證字號",
              "nullable": true
            },
            "status": {
              "$ref": "#/components/schemas/CustomerNotificationStatus"
            },
            "viewedOrRepliedTime": {
              "type": "string",
              "description": "通知查看時間/問券回覆時間",
              "format": "date-time",
              "nullable": true
            },
            "isAllowToOpenAfterReplyOrRead": {
              "type": "boolean",
              "description": "是否允許已回覆後查看 1:ALLOW; 0:DISABLE"
            },
            "createdTime": {
              "type": "string",
              "description": "通知建立時間",
              "format": "date-time"
            },
            "lastModifiedTime": {
              "type": "string",
              "description": "最後修改時間",
              "format": "date-time"
            },
            "systemID": {
              "type": "string",
              "description": "外部系統代號",
              "nullable": true
            },
            "isUseKafkaCallBack": {
              "type": "boolean",
              "description": "-- Callback 回傳方式走 kafka"
            },
            "callBack_KafkaTopicName": {
              "type": "string",
              "description": "kafka Callback Topic Name",
              "nullable": true
            },
            "callbackUrl": {
              "type": "string",
              "description": "外部系統代號回傳 網址",
              "nullable": true
            },
            "callbackHttpMethod": {
              "type": "string",
              "description": "GET|POSR",
              "nullable": true
            },
            "callbackHttpStatusCode": {
              "type": "string",
              "description": "'200'、503...etc",
              "nullable": true
            },
            "callbackCompleteTime": {
              "type": "string",
              "description": "執行Callback 時間",
              "format": "date-time"
            },
            "notificationOrReply": {
              "type": "integer",
              "description": "(通知:1/回函:2)",
              "format": "int32"
            },
            "category": {
              "type": "integer",
              "description": "類別(權益:1/帳務:2/暫停服務:3)",
              "format": "int32"
            },
            "notificationInfo": {
              "type": "string",
              "description": "通知簡述 [重要通知]缺款通知",
              "nullable": true
            },
            "requiresCertificate": {
              "type": "boolean",
              "description": "是否需要憑證簽章"
            },
            "certificateSignatureFormat": {
              "type": "string",
              "description": "簽章明文",
              "nullable": true
            },
            "surveyCakeID": {
              "type": "string",
              "description": "SurveyCakeID",
              "nullable": true
            },
            "hashKey": {
              "type": "string",
              "description": "SurveyCake HashKey",
              "nullable": true
            },
            "ivKey": {
              "type": "string",
              "description": "SurveyCake IVKey",
              "nullable": true
            },
            "notificationContent": {
              "type": "string",
              "description": "通知內容",
              "nullable": true
            },
            "replyUUID": {
              "type": "string",
              "description": "確認回覆產生一筆新UUID",
              "format": "uuid",
              "nullable": true
            },
            "answerHash": {
              "type": "string",
              "description": "cake 答案hash",
              "nullable": true
            },
            "answerResult": {
              "type": "string",
              "description": "cake 答案json",
              "nullable": true
            },
            "replyTime": {
              "type": "string",
              "description": "問券回覆時間",
              "format": "date-time",
              "nullable": true
            }
          },
          "additionalProperties": false,
          "description": "顧客通知資料"
        },
        "CustomerNotificationStatus": {
          "enum": [
            0,
            1,
            2
          ],
          "type": "integer",
          "description": "Status 待回覆/已回覆/禁止查看 0/1/2",
          "format": "int32"
        },
        "GenReplyUUIDReq": {
          "required": [
            "serialNumber"
          ],
          "type": "object",
          "properties": {
            "serialNumber": {
              "type": "string",
              "description": "通知序號",
              "format": "uuid"
            }
          },
          "additionalProperties": false
        },
        "GenReplyUUUDResp": {
          "type": "object",
          "properties": {
            "status": {
              "type": "boolean"
            },
            "message": {
              "type": "string",
              "nullable": true
            },
            "replyUUID": {
              "type": "string",
              "description": "確認回覆產生一筆新UUID",
              "format": "uuid"
            }
          },
          "additionalProperties": false
        },
        "GeneralResp": {
          "type": "object",
          "properties": {
            "status": {
              "type": "boolean"
            },
            "message": {
              "type": "string",
              "nullable": true
            }
          },
          "additionalProperties": false
        },
        "GetCustomerNotificationListResp": {
          "type": "object",
          "properties": {
            "status": {
              "type": "boolean"
            },
            "message": {
              "type": "string",
              "nullable": true
            },
            "notificationData": {
              "type": "array",
              "items": {
                "$ref": "#/components/schemas/CustomerNotification"
              },
              "description": "顧客通知列表",
              "nullable": true
            }
          },
          "additionalProperties": false
        },
        "GetItemByReplyUUIDResp": {
          "type": "object",
          "properties": {
            "status": {
              "type": "boolean"
            },
            "message": {
              "type": "string",
              "nullable": true
            },
            "data": {
              "$ref": "#/components/schemas/CustomerNotification"
            }
          },
          "additionalProperties": false
        },
        "GetSurveyCakeKeyAndIVResp": {
          "type": "object",
          "properties": {
            "status": {
              "type": "boolean"
            },
            "message": {
              "type": "string",
              "nullable": true
            },
            "key": {
              "type": "string",
              "nullable": true
            },
            "iv": {
              "type": "string",
              "nullable": true
            }
          },
          "additionalProperties": false
        },
        "InsertNotificationDataReq": {
          "required": [
            "notificationData"
          ],
          "type": "object",
          "properties": {
            "notificationData": {
              "type": "array",
              "items": {
                "$ref": "#/components/schemas/NotificationData"
              }
            }
          },
          "additionalProperties": false
        },
        "InsertNotificationResp": {
          "type": "object",
          "properties": {
            "status": {
              "type": "boolean"
            },
            "message": {
              "type": "string",
              "nullable": true
            },
            "replyData": {
              "type": "array",
              "items": {
                "$ref": "#/components/schemas/ReplyData"
              },
              "description": "新增之通知序號與通知回應網址",
              "nullable": true
            }
          },
          "additionalProperties": false
        },
        "JToken": {
          "type": "array",
          "items": {
            "$ref": "#/components/schemas/JToken"
          }
        },
        "NotificationData": {
          "required": [
            "account",
            "branch",
            "isAllowToOpenAfterReplyOrRead",
            "isUseKafkaCallBack",
            "ssn",
            "status"
          ],
          "type": "object",
          "properties": {
            "projectCode": {
              "type": "string",
              "description": "問卷內文",
              "format": "uuid",
              "nullable": true
            },
            "branch": {
              "minLength": 1,
              "type": "string",
              "description": "分公司代號 ex.8843"
            },
            "account": {
              "minLength": 1,
              "type": "string",
              "description": "帳號"
            },
            "ssn": {
              "minLength": 1,
              "type": "string",
              "description": "身分證"
            },
            "status": {
              "type": "integer",
              "description": "待回覆:0/已回覆:1",
              "format": "int32"
            },
            "isAllowToOpenAfterReplyOrRead": {
              "type": "boolean",
              "description": "是否允許已回覆後查看 1:ALLOW; 0:DISABLE"
            },
            "isUseKafkaCallBack": {
              "type": "boolean",
              "description": "-- Callback 回傳方式走 kafka"
            },
            "callBack_KafkaTopicName": {
              "type": "string",
              "description": "kafka Callback Topic Name",
              "nullable": true
            },
            "callbackUrl": {
              "type": "string",
              "description": "CallbackUrl 業務系統接收 url",
              "nullable": true
            },
            "callbackHttpMethod": {
              "type": "string",
              "description": "CallbackUrl 業務系統接收 url http method",
              "nullable": true
            },
            "notificationContent": {
              "type": "string",
              "description": "通知 內容",
              "nullable": true
            }
          },
          "additionalProperties": false
        },
        "ReplyData": {
          "type": "object",
          "properties": {
            "serialNumber": {
              "type": "string",
              "description": "新增之通知序號",
              "format": "uuid"
            },
            "replyUrl": {
              "type": "string",
              "description": "通知回應網址",
              "nullable": true
            }
          },
          "additionalProperties": false
        },
        "ResultElement": {
          "type": "object",
          "properties": {
            "subject": {
              "type": "string",
              "nullable": true
            },
            "type": {
              "type": "string",
              "nullable": true
            },
            "sn": {
              "type": "integer",
              "format": "int32",
              "nullable": true
            },
            "label": {
              "type": "string",
              "nullable": true
            },
            "alias": {
              "type": "string",
              "nullable": true
            },
            "answer": {
              "$ref": "#/components/schemas/JToken"
            },
            "otherAnswer": {
              "$ref": "#/components/schemas/JToken"
            },
            "extras": {
              "$ref": "#/components/schemas/JToken"
            },
            "answerAlias": {
              "$ref": "#/components/schemas/JToken"
            }
          },
          "additionalProperties": false
        },
        "SaveCakeAnswerReq": {
          "required": [
            "answerHash",
            "answerResult",
            "cakeAnswer",
            "notificationContent",
            "replyUUID"
          ],
          "type": "object",
          "properties": {
            "replyUUID": {
              "type": "string",
              "description": "確認回覆 識別ID",
              "format": "uuid"
            },
            "answerHash": {
              "minLength": 1,
              "type": "string",
              "description": "cake 答案hash"
            },
            "answerResult": {
              "minLength": 1,
              "type": "string",
              "description": "cake 答案json string"
            },
            "notificationContent": {
              "minLength": 1,
              "type": "string",
              "description": "通知內容"
            },
            "cakeAnswer": {
              "$ref": "#/components/schemas/SurveyCakeAnswer"
            }
          },
          "additionalProperties": false
        },
        "SaveSurveyProjectReq": {
          "required": [
            "category",
            "certificateSignatureFormat",
            "notificationInfo",
            "notificationOrReply",
            "requiresCertificate"
          ],
          "type": "object",
          "properties": {
            "notificationOrReply": {
              "type": "integer",
              "description": "(通知:1/回函:2)",
              "format": "int32"
            },
            "category": {
